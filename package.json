{
  "name": "@biothings-explorer/api-response-transform",
  "version": "1.12.0",
  "description": "a NodeJS library to transform API responses into biolink model",
  "main": "built/index.js",
  "types": "built/index.d.ts",
  "scripts": {
    "test": "npm run build && jest",
    "build": "tsc",
    "test-cov": "npm run build && jest --coverage --env node",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biothings/api-respone-transform.js.git"
  },
  "keywords": [
    "transform",
    "biolink",
    "api",
    "json"
  ],
  "author": "BioThings Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/biothings/api-respone-transform.js/issues"
  },
  "homepage": "https://github.com/biothings/api-respone-transform.js#readme",
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@types/async": "^3.2.22",
    "@types/jest": "^29.0.3",
    "@types/jsonata": "^1.5.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^20.8.7",
    "babel-jest": "^29.0.3",
    "coveralls": "^3.1.0",
    "debug": "^4.3.1",
    "jest": "^29.7.0",
    "standard-version": "^9.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^11.0.0",
    "async": "^3.2.1",
    "axios": "^0.21.1",
    "common-path-prefix": "^3.0.0",
    "husky": "^8.0.3",
    "jsonata": "^1.8.4",
    "lodash": "^4.17.21"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
